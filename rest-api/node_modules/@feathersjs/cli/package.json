  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/feathersjs/feathers/issues"
  },
  "engines": {
    "node": ">= 14"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "lib/**",
    "lib/app/static/.gitignore",
    "bin/**",
    "*.d.ts",
    "*.js"
  ],
  "scripts": {
    "prepublish": "npm run compile",
    "compile": "shx rm -rf lib/ && tsc",
